
public class ArrayTest {

	public static void main(String[] args) {
		int i = 0;
		int sum = 0;
		int[] squares = new int[101];
		while(i<=100) {
			squares[i] = i*i*i;
			System.out.println(squares[i]);
			i++;
		}
		i=0;
		while(i<squares.length) {
			sum += squares[i];
			i++;
		}
		System.out.println(sum/i);
	}

}